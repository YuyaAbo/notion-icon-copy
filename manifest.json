{
  "name": "notion-icon-copy",
  "description": "copies to clipboard uploaded icon url in Notion.",
  "version": "1.0",
  "manifest_version": 3,
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.notion.so/*"],
      "js": ["dist/content_script.js"]
    }
  ],
  "permissions": [
    "contextMenus"
  ]
}
